{
	"name": "adelarcubs/cpf-cnpj-validation",
	"require": {
		"php": ">=5.6",
		"zendframework/zend-validator": "^2.8"
	},
	"license": "BSD-3-Clause",
	"autoload": {
		"psr-4": {
			"CpfCnpjValidation\\": ["src","test"]
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^5.4",
		"squizlabs/php_codesniffer": "^2.6"
	},
	"scripts": {
		"cs": "phpcs --standard=PSR2 src test",
		"cs-fix": "phpcbf --standard=PSR2 src test"
	}
}
